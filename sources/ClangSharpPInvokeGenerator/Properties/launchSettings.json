{
  "profiles": {
    "GenerateClang": {
      "commandName": "Project",
      "commandLineArgs": "\"@$(MSBuildProjectDirectory)/Properties/GenerateClang.rsp\" --file-directory \"$(LLVMIncludePath)\" --include-directory \"$(LLVMIncludePath)\" --libraryPath $(LibClangName)",
      "nativeDebugging": true
    },
    "GenerateClangSharp": {
      "commandName": "Project",
      "commandLineArgs": "\"@$(MSBuildProjectDirectory)/Properties/GenerateClangSharp.rsp\" --file-directory \"$(MSBuildProjectDirectory)/..\" --include-directory \"$(LLVMIncludePath)\" --libraryPath $(LibClangSharpName)"
    },
    "GenerateLLVM": {
      "commandName": "Project",
      "commandLineArgs": "\"@$(MSBuildProjectDirectory)/Properties/GenerateLLVM.rsp\" --file-directory \"$(LLVMIncludePath)\" --include-directory \"$(LLVMIncludePath)\" --libraryPath $(LibLLVMName)"
    },
    "GenerateLocal": {
      "commandName": "Project",
      "commandLineArgs": "-c help"
    },
    "GenerateCpp": {
      "commandName": "Project",
      "commandLineArgs": " --file \"D:\\RidiculousClangTests\\legit_c_header.h\" -n fumocement.testbindings -o D:\\RidiculousClangTests\\outputglue.h --define-macro _ALLOW_COMPILER_AND_STL_VERSION_MISMATCH --output-mode JniGlue -m Native"
    },
    "GenerateJava": {
      "commandName": "Project",
      "commandLineArgs": " --file \"D:\\RidiculousClangTests\\legit_c_header.h\" -n fumocement.testbindings -o D:\\RidiculousClangTests\\Native.java --define-macro _ALLOW_COMPILER_AND_STL_VERSION_MISMATCH --output-mode JavaClasses -m Native"
    }
  }
}
